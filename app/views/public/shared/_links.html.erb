<%- if controller_name != 'sessions' %>
  <%= link_to "ユーザー登録がお済みの方はこちら", new_session_path(resource_name), class: "text-secondary" %><br />
<% end %>

<%- if devise_mapping.registerable? && controller_name != 'registrations' %>
  <%= link_to "ユーザー登録されていない方はこちら", new_registration_path(resource_name), class: "text-secondary" %><br />
<% end %>